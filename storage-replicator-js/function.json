{
  "disabled": false,
  "bindings": [
    {
      "name": "inputBlob",
      "type": "blobTrigger",
      "direction": "in",
      "path": "input/{name}",
      "connection": "inputStorageConnection"
    },
    {
      "name": "outputBlob",
      "type": "blob",
      "path": "jsOutput/{name}",
      "connection": "outputStorageConnection",
      "direction": "out"
    }
  ]
}