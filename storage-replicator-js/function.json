{
  "disabled": false,
  "bindings": [
    {
      "name": "inputBlob",
      "type": "blobTrigger",
      "direction": "in",
      "path": "%InputPath%/{name}",
      "connection": "InputStorageConnection"
    },
    {
      "name": "outputBlob",
      "type": "blob",
      "path": "%OutputPath%/{name}",
      "connection": "OutputStorageConnection",
      "direction": "out"
    }
  ]
}